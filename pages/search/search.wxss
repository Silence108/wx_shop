/* pages/search/search.wxss */
page {
  background-color: #fff;
}

/* 最近搜索 */
.attr {
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  padding: 0 30rpx;
  line-height: 88rpx;
}
.attr image {
  width: 24rpx;
  height: 26rpx;
  margin-top: 30rpx;
}
.itembox {
  display: flex;
  flex-wrap: wrap;
  padding: 0 26rpx;
  font-size: 26rpx;
}
.itembox .item {
  background: #f5f5f5;
  border-radius: 60rpx;
  line-height: 60rpx;
  padding: 0 30rpx;
  margin-right: 12rpx;
  margin-bottom: 20rpx;
}